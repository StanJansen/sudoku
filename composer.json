{
    "name": "stanjan/sudoku",
    "type": "library",
    "description": "Sudoku generators, solvers and readers.",
    "keywords": [
        "Sudoku",
        "Generator",
        "Solver",
        "Reader",
        "OpenCV",
        "OCR"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stan Jansen",
            "email": "stanjan@live.nl"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.64",
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "ext-curl": "Enables cURL for Guzzle"
    },
    "autoload": {
        "psr-4": {
            "Stanjan\\Sudoku\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stanjan\\Sudoku\\Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
